<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_upcoming_event">

    <fragment
        android:id="@+id/navigation_upcoming_event"
        android:name="com.dicoding.dicodingevent.ui.UpcomingEvent.UpcomingEventFragment"
        android:label="Upcoming"
        tools:layout="@layout/fragment_upcoming_event" />

    <fragment
        android:id="@+id/navigation_finished_event"
        android:name="com.dicoding.dicodingevent.ui.finishedEvent.FinishedEventFragment"
        android:label="Finished Events"
        tools:layout="@layout/fragment_finished_event" />

    <fragment
        android:id="@+id/navigation_bookmark_event"
        android:name="com.dicoding.dicodingevent.ui.bookmarkEvent.BookmarkFragment"
        android:label="Bookmark Events"
        tools:layout="@layout/fragment_bookmark" />

    <fragment
        android:id="@+id/navigation_setting"
        android:name="com.dicoding.dicodingevent.ui.setting.SettingFragment"
        android:label="Setting"
        tools:layout="@layout/fragment_setting2" />


</navigation>